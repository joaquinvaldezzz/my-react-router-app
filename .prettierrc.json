{
  "printWidth": 100,
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "^(react/(.*)$)|^(react$)",
    "^(react-router/(.*)$)|^(react-router$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/config/(.*)$",
    "^@/lib/(.*)$",
    "^@/hooks/(.*)$",
    "^@/components/ui/(.*)$",
    "^@/components/(.*)$",
    "^@/styles/(.*)$",
    "",
    "<TYPES>",
    "<TYPES>^@/types/(.*)$",
    "<TYPES>^[./]",
    "",
    "^[./]"
  ],
  "tailwindStylesheet": "./app/app.css",
  "tailwindFunctions": ["tv"]
}
